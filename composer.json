{
  "name": "steadlane/silverstripe-cloudflare",
  "description": "This module aims to relieve the stress of using Cloudflare caching with any SilverStripe project. Adds extension hooks that clears Cloudflare's cache for a specific page when that page is published or unpublished.",
  "type": "silverstripe-vendormodule",
  "version": "2.0.0",
  "keywords": [
    "cache",
    "cloudflare",
    "silverstripe"
  ],
  "license": "BSD (3-Clause)",
  "authors": [
    {
      "name": "Reece Alexander",
      "email": "support@steadlane.com.au"
    },
    {
      "name": "Andrew Pike",
      "email": "support@steadlane.com.au"
    }
  ],
  "support": {
    "issues": "https://github.com/steadlane/silverstripe-cloudflare/issues"
  },
  "require": {
    "php": "^5.6 || ^7",
    "ext-json": "*",
    "ext-curl": "*",
    "psr/cache": "*",
    "silverstripe/vendor-plugin": "^1",
    "silverstripe/framework": "^4",
    "silverstripe/cms": "^4"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "installer-name": "silverstripe-cloudflare",
    "expose": [
      "css",
      "assets"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/steadlane/silverstripe-cloudflare"
    }
  ]
}
